{
  "name": "sdjs-pipeline",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=8.16"
  },
  "scripts": {
    "lint": "eslint .",
    "build": "npm run build:sass && webpack --p",
    "build:sass": "sass src/css/style.scss dist/style.css",
    "ci": "start-server-and-test start http://localhost:3000 cy:run",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel watch:server build",
    "sass": "sass --watch src/css/style.scss dist/style.css",
    "start": "node .",
    "test": "cross-env NODE_ENV=development npm-run-all --parallel ci test:unit",
    "test:unit": "mocha --require ignore-styles test/setup.js test/**/**.spec.js --exit",
    "watch:server": "nodemon server/server.js"
  },
  "dependencies": {
    "@babel/register": "^7.0.0",
    "@sendgrid/mail": "^6.3.1",
    "axios": "^0.18.0",
    "chai-as-promised": "^7.1.1",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "dotenv": "^6.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "helmet": "^3.10.0",
    "loopback": "^3.19.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^6.0.0",
    "loopback-connector-mongodb": "^3.8.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "nightmare": "^3.0.1",
    "ontime": "0.0.6",
    "react": "^16.5.2",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-redux-form": "^1.16.10",
    "react-router": "^4.3.0",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-promise-middleware": "^5.1.0",
    "redux-thunk": "^2.3.0",
    "sass": "^1.14.3",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0-beta.6",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cross-env": "^5.1.4",
    "cypress": "^3.2.0",
    "eslint": "^3.17.1",
    "eslint-config-loopback": "^8.0.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^6.1.2",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.1",
    "start-server-and-test": "^1.7.13",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sandiegojs/sdjs-speaker-pipeline"
  },
  "keywords": [
    "sdjs",
    "san-diego",
    "javascript",
    "women-of-sdjs"
  ],
  "license": "MIT",
  "description": "sdjs-pipeline"
}
